{
  "name": "ai-agent-studio-mobile",
  "version": "1.0.0",
  "description": "Complete AI-Powered Creative Suite for Mobile - Generate videos, apps, music, and more with free open-source AI models",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios", 
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:apk": "eas build --platform android --profile preview",
    "dev": "concurrently \"npm run backend:dev\" \"npm run mobile:dev\"",
    "backend:dev": "cd backend && python -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "mobile:dev": "cd mobile-app && expo start",
    "install:all": "npm install && cd mobile-app && npm install && cd ../backend && pip install -r requirements.txt",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "deploy:huggingface": "cd huggingface-spaces && git push origin main",
    "build:docker": "docker-compose build",
    "start:docker": "docker-compose up -d",
    "generate:apk": "cd mobile-app && eas build --platform android --profile preview --local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johnshah/ai-agent-studio-mobile-.git"
  },
  "keywords": [
    "ai",
    "mobile-app",
    "video-generation", 
    "app-development",
    "music-generation",
    "react-native",
    "fastapi",
    "huggingface",
    "stable-diffusion",
    "wan2.2",
    "musicgen",
    "open-source",
    "free"
  ],
  "author": "Johnshah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Johnshah/ai-agent-studio-mobile-/issues"
  },
  "homepage": "https://github.com/Johnshah/ai-agent-studio-mobile-#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  }
}